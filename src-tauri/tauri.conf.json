{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "logicui",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "path": {
        "all": true
      },
      "dialog": {
        "all": true,
        "ask": false,
        "confirm": false,
        "message": false,
        "open": false,
        "save": false
      },
      "all": false,
      "fs": {
        "all": true,
        "readDir": true,
        "readFile": true,
        "writeFile": true,
        "createDir": true,
        "scope": ["$APPDATA/*"]
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "LogicUI",
        "width": 800,
        "height": 600,
        "minHeight": 600,
        "minWidth": 800
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.logicui.quimera",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.ico"
      ]
    }
  }
}
